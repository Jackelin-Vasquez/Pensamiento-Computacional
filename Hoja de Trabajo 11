import math
from abc import abc, abstractmethod

class ExperimentoFisico(abc):
    @abstractmethod
    def realizar_experimento(self)
    pass

class caidalibre(ExperimentoFisico):
    def __init__(self, altura,gravedad)
    self.altura =altura
    sel.gravedad=gravedad
    
    def realizar_experimento(self):
        if sel.altura <0 or self.gravedad <0:

            raise print("La altura o tiempo no puede ser menor a 0")
            
        tiempo= math.sqrt( (2*self.altura)/ self.gravedad))
        return tiempo
        
    if __name__="__name__":
        print("---SIMULACIÃ“N DE CAIDAD LIBRE---")
        try:
            altura= int(input("Ingrese altura del objeto"))
            gravedad= int(input("Ingrese gravedad"))
            
            experimento= caidalibre(altura,gravedad)
